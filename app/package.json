{
  "name": "lightbulb-learning",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "build": "npm run genTypes && svelte-kit build",
    "seed": "psql 'postgresql://postgres:postgres@localhost:54322/postgres' -f supabase/seed.sql",
    "test": "npm run seed && npx playwright test",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "compile": "tsc",
    "genTypes": "npx openapi-typescript 'https://rkpsxesguhibbzhylmsd.supabase.co/rest/v1/?apikey=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJyb2xlIjoiYW5vbiIsImlhdCI6MTY0MTM4NTMyNCwiZXhwIjoxOTU2OTYxMzI0fQ.omjbj_RDCOBlGNQqJ_sXjOuschrmCA4aOp9k4ajvxL8' > src/lib/models/supabase.ts",
    "genTypesLocal": "npx openapi-typescript 'http://localhost:54321/rest/v1/?apikey=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6ImFub24ifQ.625_WdcF3KHqz5amU0x2X5WWHP-OEs_4qj0ssLNHzTs' > src/lib/models/supabase.ts",
    "dumpSchema": "dotenv -e .env-prod -- bash -c 'pg_dump --host=$DB_HOST --username=$DB_USER --schema-only --schema=public --no-password > supabase/seed.sql '",
    "sb:start": "supabase start",
    "sb:stop": "supabase stop"
  },
  "devDependencies": {
    "@playwright/test": "^1.19.2",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-replace": "^3.0.1",
    "@sveltejs/adapter-auto": "^1.0.0-next.22",
    "@sveltejs/kit": "^1.0.0-next.266",
    "@types/cookie": "^0.4.1",
    "@typescript-eslint/eslint-plugin": "^5.7.0",
    "@typescript-eslint/parser": "^5.7.0",
    "@zerodevx/svelte-toast": "^0.6.2",
    "autoprefixer": "^10.4.0",
    "concurrently": "^7.0.0",
    "cssnano": "^5.0.13",
    "dotenv": "^10.0.0",
    "dotenv-cli": "^4.1.1",
    "eslint": "^8.4.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "jsonwebtoken": "^8.5.1",
    "postcss": "^8.4.5",
    "postcss-load-config": "^3.1.0",
    "prettier": "^2.5.1",
    "prettier-plugin-svelte": "^2.5.1",
    "supabase-ui-svelte": "^0.1.1",
    "svelte": "^3.44.3",
    "svelte-check": "^2.2.10",
    "svelte-preprocess": "^4.10.1",
    "tailwindcss": "^3.0.6",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4"
  },
  "type": "module",
  "dependencies": {
    "@supabase/auth-helpers-svelte": "^0.4.2",
    "@supabase/auth-helpers-sveltekit": "^0.6.2",
    "@supabase/supabase-js": "^1.29.1",
    "autosize": "^5.0.1",
    "cookie": "^0.4.2",
    "object-key-convert": "^0.0.10",
    "playwright": "^1.19.2",
    "process": "^0.11.10"
  }
}
